{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2023",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "@app/auth": [
        "libs/auth/src"
      ],
      "@app/auth/*": [
        "libs/auth/src/*"
      ],
      "@app/bets": [
        "libs/bets/src"
      ],
      "@app/bets/*": [
        "libs/bets/src/*"
      ],
      "@app/core": [
        "libs/core/src"
      ],
      "@app/core/*": [
        "libs/core/src/*"
      ],
      "@app/dashboard": [
        "libs/dashboard/src"
      ],
      "@app/dashboard/*": [
        "libs/dashboard/src/*"
      ],
      "@app/games": [
        "libs/games/src"
      ],
      "@app/games/*": [
        "libs/games/src/*"
      ],
      "@app/transactions": [
        "libs/transactions/src"
      ],
      "@app/transactions/*": [
        "libs/transactions/src/*"
      ],
      "@app/typeorm": [
        "libs/typeorm/src"
      ],
      "@app/typeorm/*": [
        "libs/typeorm/src/*"
      ],
      "@app/users": [
        "libs/users/src"
      ],
      "@app/users/*": [
        "libs/users/src/*"
      ]
    }
  }
}