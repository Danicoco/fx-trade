      "compilerOptions": {
        "tsConfigPath": "libs/core/tsconfig.lib.json"
      }
    },
    "dashboard": {
      "type": "library",
      "root": "libs/dashboard",
      "entryFile": "index",
      "sourceRoot": "libs/dashboard/src",
      "compilerOptions": {
        "tsConfigPath": "libs/dashboard/tsconfig.lib.json"
      }
